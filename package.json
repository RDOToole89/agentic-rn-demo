{
  "name": "agentic-rn-demo",
  "private": true,
  "scripts": {
    "dev": "pnpm -C apps/client start",
    "lint": "pnpm -r run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "pnpm -r run typecheck",
    "generate:api": "orval",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.11.0",
    "orval": "^8.5.0",
    "prettier": "^3.4.0",
    "typescript": "^5.6.0"
  },
  "pnpm": {
    "overrides": {
      "lightningcss": "1.30.1"
    }
  }
}
